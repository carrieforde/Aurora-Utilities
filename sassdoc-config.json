      "01-icons": "Icons",
      "01-typography": "Typography",
      "01-widths": "Widths",
      "01-z-index": "Z Indeces"
    },
    "Functions": {
      "02-breakpoint": "Breakpoint",
      "02-color": "Color",
      "02-size": "Size",
      "02-z-index": "Z Index"
    },
    "Mixins": {
      "03-colors": "Colors",
      "03-buttons": "Buttons",
      "03-links": "Links",
      "03-media-queries": "Media Queries",
      "03-utilities": "Utilities",
      "03-positioning": "Positioning",
      "03-lists": "Lists"
    }
  },
  "herman": {
    "displayColors": ["hex"],
    "customCSS": "dist/sassdoc.css",
