{
  "name": "aurora-utilities",
  "version": "2.0.1",
  "description": "Sass and JavaScript utilities for all your front end projects.",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --watch",
    "format": "prettier --write \"**/**/*.{js,jsx,scss}\"",
    "sassdoc": "sassdoc sass -c sassdoc-config.json",
    "stylelint": "stylelint \"**/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"**/**/*.{css,scss}\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carrieforde/Aurora-Utilities.git"
  },
  "keywords": [
    "sass",
    "utilities"
  ],
  "author": "Carrie Forde <carrie@carrieforde.com> (https://carrieforde.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/carrieforde/Aurora-Utilities/issues"
  },
  "homepage": "https://github.com/carrieforde/Aurora-Utilities#readme",
  "devDependencies": {
    "css-loader": "^3.4.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "sassdoc": "^2.7.1",
    "sassdoc-theme-herman": "^3.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.14.2",
    "stylelint-webpack-plugin": "^1.2.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "npm": "^6.14.2"
  }
}
